{
  "name": "backend",
  "author": "quenoftroll",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "(npm run db:setup || npm run db:up) && node ./build/index.js",
    "build": "tsc -p .",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "npx eslint . --ext .ts",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "db:setup": "npx sequelize db:create && npx sequelize db:migrate",
    "db:up": "npx sequelize db:migrate",
    "db:down": "npx sequelize db:migrate:undo",
    "db:restart": "npx sequelize db:drop && npm run db:setup",
    "test": "jest"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node-cron": "^3.0.8",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.6.4",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "fastest-validator": "^1.16.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.5.0",
    "node-cron": "^3.0.2",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.7",
    "sequelize": "^6.28.2",
    "sequelize-cli": "^6.6.0"
  }
}
